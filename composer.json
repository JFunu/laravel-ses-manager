{
    "name": "Jfunu/laravel-ses-manager",
    "description": "Logs SES bounces and complaints for Laravel app",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Jfunu",
            "email": "jfunu79@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jfunu\\LaravelSesManager\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfunu\\LaravelSesManager\\Tests\\": "./tests"
        }
    },
    "require": {
        "aws/aws-php-sns-message-validator": "^1.5",
        "illuminate/support": "^5.8 | ^6.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jfunu\\LaravelSesManager\\LaravelSesEventsServiceProvider"
            ]
        }
    }
}
